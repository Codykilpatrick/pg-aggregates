{
  "name": "@graphile/pg-aggregates",
  "version": "0.2.0-0.2",
  "description": "Aggregates support for PostGraphile",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest -i",
    "tslint": "prettier --list-different 'src/**/*' && tslint --config tslint.json --project tsconfig.json",
    "dev": "psql -X1v ON_ERROR_STOP=1 -f __tests__/schema.sql graphile_aggregates && concurrently --kill-others 'yarn run watch' 'yarn nodemon --watch dist -x postgraphile'",
    "prepack": "rm -Rf dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/pg-aggregates.git"
  },
  "keywords": [
    "postgraphile",
    "graphile",
    "plugin",
    "aggregates",
    "pg",
    "postgres",
    "postgresql",
    "graphql"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/pg-aggregates/issues"
  },
  "homepage": "https://github.com/graphile/pg-aggregates#readme",
  "peerDependencies": {
    "graphile-build": "^5.0.0-0.17",
    "graphile-build-pg": "^5.0.0-0.20",
    "graphql": "^16.1.0-experimental-defer-stream.6"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "graphile-build": "^5.0.0-0.18",
    "graphile-build-pg": "^5.0.0-0.21",
    "graphql": "^16.1.0-experimental-defer-stream.6",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "postgraphile": "^5.0.0-0.23",
    "postgraphile-plugin-connection-filter": "^3.0.0-0.1",
    "prettier": "^2.8.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.9",
    "debug": "^4.3.4",
    "graphile-utils": "^5.0.0-0.21"
  },
  "files": [
    "dist"
  ]
}
